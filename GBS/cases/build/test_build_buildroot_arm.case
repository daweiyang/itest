__Steps__

> if [ -d ~/buildroot/local/repos ];then sudo rm ~/buildroot/local/repos -rf; fi

> if [ -d ./build ];then rm build -rf; fi

> mkdir build

> cd build

> ../tools/create_proj ../specs/tizen-release.spec

> ../tools/create_proj ../specs/fake.spec

> ../tools/create_proj ../specs/xorg-x11-filesystem.spec

> gbs -c ../conf_fixtures/tizendev.cfg build --buildroot=~/buildroot -A armv7l 2>&1 | tee log

> test -z "`grep "mount:.*scratch" log`"

> cd tizen-release && test -n "`git status | grep "working directory clean"`" && cd ..

> cd fake && test -n "`git status | grep "working directory clean"`" && cd ..

> cd xorg-x11-filesystem && test -n "`git status | grep "working directory clean"`" && cd ..

> test -n "`find ~/buildroot/local/repos -name "*fake*.rpm"`"

> test -n "`find ~/buildroot/local/repos -name "*tizen-release*.rpm"`"

> test -n "`find ~/buildroot/local/repos -name "*xorg-x11-filesystem*.rpm"`"

