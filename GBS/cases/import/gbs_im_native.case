__Steps__

> ./tools/create_proj ./specs/fake.spec

> cd fake

> gbs export --source-rpm

> cp `find packaging/ -name '*.src.rpm'` ../

> cd ..

> rm -rf fake

> gbs im fake-1.0-1.src.rpm --native

> cd fake

#no other branch
> test -z $(git branch | grep -v master)

#no other tag
> test -z $(git tag | grep -v vendor/1.0-1)

